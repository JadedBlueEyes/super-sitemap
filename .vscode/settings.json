{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/.svelte-kit": true
  },
  "cSpell.words": [
    "changefreq",
    "datetime",
    "devs",
    "lastmod",
    "loc",
    "prerender",
    "prerendering",
    "publint",
    "signup",
    "sitemapindex",
    "subpage",
    "subpages"
  ],
  "cSpell.ignoreWords": []
}
